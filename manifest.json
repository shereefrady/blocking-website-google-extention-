{
  "manifest_version": 2,
  "name": "Print this page",
  "description": "Adds a print button to the browser.",
  "version": "1.0",
  
  "permissions":  [
    "activeTab",
     "storage",
     "desktopCapture",
     "tabCapture"
    ],
  "browser_action": {
      "default_title": "Print this page",
      "default_popup" : "popup.html" ,
      "default_icons" : "get_started16.png" 
  }
  
}
